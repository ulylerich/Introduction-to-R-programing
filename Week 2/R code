lightspeed <- read.csv("https://raw.githubusercontent.com/vincentarelbundock/Rdatasets/master/csv/gamclass/cvalues.csv",header=TRUE, na.strings="NA", sep=",") 
print(lightspeed) 
nrow(lightspeed) 
ncol(lightspeed) 
class(lightspeed) 
lapply(lightspeed,class) 
lightspeed1<-na.omit(lightspeed) 
print(lightspeed1) 
write.table(x=lightspeed1,file = "/Users/uadegbola/Desktop/lightspeed2.csv", quote=FALSE, col.names =TRUE,sep = " ") 
read.csv ("lightspeed2.csv",header= TRUE, sep = " ") 
Pargon<read.csv("https://raw.githubusercontent.com/vincentarelbundock/Rdatasets/master/csv/DAAG/hotspo ts.csv",header=TRUE, na.strings="NA", sep=",") 
print(Pargon) 
nrow(Pargon) 
ncol(Pargon) 
lapply(Pargon,class) 
min(Pargon$distance) 
mean(Pargon$error) 
Pargon1<-Pargon[c("name","source")] 
print(Pargon1) 
write.table(x=Pargon1,file = "/Users/uadegbola/Desktop/Pargon2.csv", quote=FALSE, row.names =FALSE,sep = ",")
